<div data-analytics-viewport="autotune"
     data-analytics-label="<%=@project.slug %>:<%=@project.id %>"
     id="<%=@project.slug %>__graphic">
<% if @project.blueprint_config['partner_platform_embed_type'] && @project.blueprint_config['partner_platform_embed_type'] === 'image' %>
  <img class="partner-platform" src='<%= @project.deployer(:publish).url_for('screenshots/screenshot_s.png') %>' width="400px" height="500px" />
<% end %>
</div>
<script type="text/javascript">
(function() {
  <% if @project.blueprint_config['partner_platform_embed_type'] && @project.blueprint_config['partner_platform_embed_type'] === 'image' %>
    document.getElementsByClassName('partner-platform')[0].style.display = 'none';
  <% end %>
  var l = function() {
    new pym.Parent(
      '<%=@project.slug %>__graphic',
      '<%=@project.deployer(:publish).url_for('/') %>');
  };
  if(typeof(pym) === 'undefined') {
    var h = document.getElementsByTagName('head')[0],
        s = document.createElement('script');
    s.type = 'text/javascript';
    s.src = 'https://cdnjs.cloudflare.com/ajax/libs/pym/0.4.5/pym.js';
    s.onload = l;
    h.appendChild(s);
  } else {
    l();
  }
})();
</script>
